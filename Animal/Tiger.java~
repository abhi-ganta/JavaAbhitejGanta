public class Tiger extends Kitty{
  private String talk;
  
  public Tiger(String name, String food, boolean poop, boolean isAPet, String t)
  {
    super(name, food, poop, isAPet);
    talk = t;
  }
  public String getTalk(){
    return "I'am Tigger, child of Kitty";
  }
  public String toString()
  {
    return super.toString() +getTalk() + "\n";
  }
}
  